<% layout("layouts/boilerplate") -%>

<div class="row">
  <div class="col-md-6 offest-3">
    <div class="alert alert-danger" role="alert">
      <h4 class="alert-heading"><%=err.message %></h4>
      <p><%= err.status %></p>
